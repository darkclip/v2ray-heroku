{
  "name": "V2ray",
  "description": "deploy V2ray to Heroku.",
  "keywords": ["V2ray"],
  "env": {
    "VER": {
       "description": "输入v2ray Core 版本号（latest表示最新版本）",
       "value": "latest"
     }, 
    "UUID1": {
       "description": "Vmess协议UUID，请输入自己的UUID",
       "value": "00000000-0000-0000-0000-000000000000"
     },
    "UUID2": {
       "description": "Vmess协议UUID，请输入自己的UUID",
       "value": "00000000-0000-0000-0000-000000000000"
     },
    "UUID3": {
       "description": "Vmess协议UUID，请输入自己的UUID",
       "value": "00000000-0000-0000-0000-000000000000"
     },
    "UUID4": {
       "description": "Vmess协议UUID，请输入自己的UUID",
       "value": "00000000-0000-0000-0000-000000000000"
     },
    "V2RAY_RAY_BUFFER_SIZE": {
       "description": "每个连接的缓存大小，单位MB",
       "value": "10"
     }
  },
  "website": "https://github.com/wangyi2005/v2ray-heroku",
